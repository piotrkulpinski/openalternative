    "@remix-run/dev": "^2.11.1",
    "@tailwindcss/typography": "0.5.10",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vercel/remix": "^2.11.1",
    "autoprefixer": "10.4.18",
    "postcss": "8.4.37",
