{
  "name": "openalternative",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "lint": "bun biome lint --apply .",
    "check": "bun biome check --apply .",
    "format": "bun biome format --organize-imports-enabled --write .",
    "typecheck": "tsc",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev --name ",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push --skip-generate",
    "db:reset": "prisma migrate reset",
    "postinstall": "bun run db:generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/lib-storage": "^3.540.0",
    "@curiousleaf/utils": "^1.0.22",
    "@epic-web/cachified": "^5.1.2",
    "@epic-web/remember": "^1.0.2",
    "@octokit/graphql": "^8.0.1",
    "@prisma/client": "^5.13.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/node": "^2.10.0",
    "@remix-run/react": "^2.10.0",
    "@remix-run/serve": "^2.10.0",
    "@resvg/resvg-js": "^2.6.2",
    "@uidotdev/usehooks": "^2.4.1",
    "ai": "^3.2.14",
    "algoliasearch": "^4.23.2",
    "cva": "beta",
    "date-fns": "^3.6.0",
    "got": "^14.2.1",
    "inngest": "^3.16.1",
    "instantsearch.js": "^4.66.1",
    "isbot": "^4.1.0",
    "lucide-react": "^0.359.0",
    "next-themes": "^0.3.0",
    "plur": "^5.1.0",
    "posthog-js": "^1.116.6",
    "query-string": "^9.0.0",
    "react": "^18.3.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.0",
    "react-instantsearch": "^7.7.1",
    "react-instantsearch-core": "^7.7.1",
    "react-markdown": "^9.0.1",
    "remix-utils": "^7.5.0",
    "satori": "^0.10.13",
    "stripe": "^15.3.0",
    "tailwind-merge": "^2.2.2",
    "timeago.js": "^4.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.1",
    "@remix-run/dev": "^2.10.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vercel/remix": "^2.9.2",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.37",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.1.6",
    "vite": "5.1.7",
    "vite-plugin-node-polyfills": "^0.21.0",
